    "components": {
        "schemas": {
            "Chat": {
                "required": [
                    "content",
                    "model"
                ],
                "type": "object",
                "properties": {
                    "model": {
                        "type": "string"
                    },
                    "content": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ContentEx"
                        }
                    },
                    "chatId": {
                        "type": "string"
                    }
                }
            },
            "ContentEx": {
                "required": [
                    "type"
                ],
                "type": "object",
                "properties": {
                    "input": {
                        "type": "string"
                    },
                    "inputReference": {
                        "type": "string"
                    },
                    "type": {
                        "pattern": "text|audio|doc|image|video",
                        "type": "string"
                    }
                }
            },
            "UsageCalculationEx": {
                "type": "object",
                "properties": {
                    "estimatedTokens": {
                        "type": "integer",
                        "format": "int32"
                    }
                }
            },
            "ModelEx": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "clientId": {
                        "type": "string"
                    },
                    "proprietary": {
                        "type": "boolean"
                    },
                    "baseModel": {
                        "type": "string"
                    },
                    "baseModelId": {
                        "type": "string"
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updatedAt": {
                        "type": "string",
                        "format": "date-time"
                    }
                }
            },
            "Embed": {
                "required": [
                    "content",
                    "model",
                    "purpose"
                ],
                "type": "object",
                "properties": {
                    "model": {
                        "type": "string"
                    },
                    "content": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ContentEx"
                        }
                    },
                    "purpose": {
                        "pattern": "search_document|search_query|classification|clustering",
                        "type": "string"
                    }
                }
            },
            "EmbedResponseEx": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "usage": {
                        "$ref": "#/components/schemas/UsageEx"
                    }
                }
            },
            "TokensEx": {
                "type": "object",
                "properties": {
                    "inputTokens": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "outputTokens": {
                        "type": "integer",
                        "format": "int32"
                    }
                }
            },
            "UsageEx": {
                "type": "object",
                "properties": {
                    "billedTokens": {
                        "$ref": "#/components/schemas/TokensEx"
                    },
                    "usedTokens": {
                        "$ref": "#/components/schemas/TokensEx"
                    }
                }
            },
            "ChatResponseEx": {
                "type": "object",
                "properties": {
                    "output": {
                        "type": "string"
                    },
                    "confidenceScore": {
                        "type": "number"
                    },
                    "model": {
                        "type": "string"
                    },
                    "usage": {
                        "$ref": "#/components/schemas/UsageEx"
                    }
                }
            },
            "TotalUsageEx": {
                "type": "object",
                "properties": {
                    "totalInput": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "totalOutput": {
                        "type": "integer",
                        "format": "int32"
                    }
                }
            },
            "PaginatedResponseExListModelEx": {
                "type": "object",
